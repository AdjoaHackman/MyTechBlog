<!-- Loop over each user object -->
{{#each posts as |post| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <!-- Render user-specific data with Handlebars.js expressions -->
    <h2>{{{get_emoji}}} {{post.username}}</h2>
    <h2>{{{get_emoji}}} {{post.title}}</h2>
    <h2>{{{get_emoji}}} {{post.dateCreation}}</h2>
    <h2>{{{get_emoji}}} {{post.content}}</h2>
    
  </div>
  <div class="col-md-6">
  </div>
</div>
{{/each}}
